{
  "name": "electron-react-boilerplate",
  "version": "0.0.0",
  "description": "A highly opinionated boilerplate that is extremely customizable for starting projects with Electron and React!",
  "keywords": [
    "airbnb",
    "babel",
    "boilerplate",
    "electron",
    "eslint",
    "hot module reload",
    "postcss",
    "prettier",
    "react",
    "react refresh",
    "redux",
    "scss",
    "stylelint",
    "webpack"
  ],
  "homepage": "https://github.com/shift4id/electron-react-boilerplate",
  "bugs": {
    "url": "https://github.com/shift4id/electron-react-boilerplate/issues",
    "email": "hi@sid.fyi"
  },
  "license": "MIT",
  "author": {
    "name": "Siddharth Adusumelli",
    "email": "hi@sid.fyi",
    "url": "https://github.com/shift4id"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shift4id/electron-react-boilerplate.git"
  },
  "build": {
    "appId": "com.shift4id.electron-react-boilerplate",
    "files": [
      "build/*",
      "node_modules/**/*"
    ],
    "productName": "Electron React Boilerplate"
  },
  "scripts": {
    "webpack": "rimraf build && cross-env NODE_ENV=development webpack",
    "build": "rimraf build && cross-env NODE_ENV=production webpack",
    "dev": "yarn webpack && yarn renderer:dev",
    "start": "yarn main:prod",
    "lint:js": "eslint .",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:css": "stylelint \"**/*.{css,scss}\"",
    "lint:css:fix": "yarn lint:css --fix",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:fix": "yarn lint:js:fix && yarn lint:css:fix",
    "main:dev": "cross-env ELECTRON_ENV=dev NODE_ENV=development run-electron ./build/app.js",
    "main:prod": "cross-env ELECTRON_ENV=prod NODE_ENV=production run-electron ./build/app.js",
    "renderer:dev": "cross-env ELECTRON_ENV=dev NODE_ENV=development webpack serve",
    "renderer:prod": "cross-env ELECTRON_ENV=prod NODE_ENV=production webpack serve",
    "package:all": "yarn build && electron-builder -mwl --publish never",
    "package:lin": "yarn build && electron-builder -l --publish never",
    "package:mac": "yarn build && electron-builder -m --publish never",
    "package:win": "yarn build && electron-builder -w --publish never"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "electron-store": "^7.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-electron-storage": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@svgr/webpack": "^5.5.0",
    "babel-loader": "^8.2.2",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "electron": "^11.1.1",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.2",
    "html-webpack-plugin": "^5.1.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.7",
    "postcss": "^8.2.6",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^2.2.1",
    "react-refresh": "^0.9.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "run-electron": "^1.0.0",
    "stylelint": "^13.11.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "private": false
}
